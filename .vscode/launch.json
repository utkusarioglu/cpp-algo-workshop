  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug test",
      "program": "${workspaceRoot}/build/linux/debug/bin/cpp-algo-workshop-test",
      "request": "launch",
      "type": "cppdbg",
      "cwd": "${workspaceRoot}",
      "linux": {
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
      ],
      },
      "preLaunchTask": "...Debug Linux",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false
    }
  ]
}
